class ChildThread implements Runnable {
    public void run()
    {
        for(int i=1;i<=10;++i){
            System.out.println("ChildThread");
        }
    }
    
}
class RunnableInterfaceThread{
    public static void main(String args[]){
        Thread t = new Thread();
        t.start();
        t.run();
        for(int i=1;i<=10;++i){
           System.out.println("MAIN Thread");
        }
    }
}
